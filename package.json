{
  "name": "aquariusro",
  "version": "1.0.0",
  "private": true,
  "workspaces": ["frontend", "backend"],
  "type": "module",
  "scripts": {
    "start": "concurrently \"npm run dev-server\" \"npm run dev --prefix frontend\"",
    "dev-server": "npm run dev --prefix backend",
    "server": "npm run start --prefix backend",
    "client": "npm run dev --prefix frontend",
    "install-all": "npm install --workspaces"
  },
  "dependencies": {
    "concurrently": "^8.2.2"
  }
}





